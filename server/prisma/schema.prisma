generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model UserProfile {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  age       Int
  address   String
  startDate DateTime
  password  String
  picture   String  
}


model Todo_title {
  titleId      Int   @id @default(autoincrement()) 
  title      String
}

